    "eslint-config-brightspace": "^0.16",
    "eslint-plugin-html": "^6",
    "eslint-plugin-import": "^2",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "sinon": "^14"
  },
  "files": [
    "d2l-file-uploader.js",
    "file-uploader.serge.json",
    "localize-behavior.js",
